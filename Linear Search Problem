#include<iostream>
using namespace std;
int linearsearch(int arr[], int size, int target){
   for(int i=0; i<size ;i++){
       if(arr[i]==target){
            return i;
      }
    }
    return -1;
 }
int main(){
   int arr[]={1,3,5,9,19,100,109,110,117,150};
   int size=10;
   int target=100;
   linearsearch(arr, size, target);
}
