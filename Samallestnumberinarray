#include<iostream>
#include<climits>
using namespace std;
int main(){
  int size=5;
  int nums[size];
  for(int i=0;i<size;i++){
     cout<<"Please enter the value of array nums:";
     cin>>nums;
  }
  int smallest=INT_MAX;
  for(int i=0;i<size;i++){
     if(nums[i]<smallest){
         smallest=nums[i];
      }
   }
   cout<<smallest;
}
